<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
             xmlns:b.html='urn:import:org.gwtbootstrap3.client.ui.html'
>

	<ui:style>
		.center { margin-top: 20px; }
	</ui:style>

	<b.html:Div>

		<b:Heading size="H2" ui:field="pageTitle">
			<b:Icon ui:field="icon" type="EXCLAMATION_TRIANGLE" fixedWidth="true" />
			<b.html:Text text="You are not registered user" ui:field="text"/>
			<b.html:Small text=""/>
		</b:Heading>

		<b:Alert type="WARNING" ui:field="message" dismissable="false">
		</b:Alert>

		<b:Button ui:field="button" visible="false" text="Continue" icon="CHEVRON_RIGHT" iconPosition="RIGHT" iconFixedWidth="true" type="SUCCESS" addStyleNames="{style.center}"/>

	</b.html:Div>


</ui:UiBinder>