<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:b='urn:import:org.gwtbootstrap3.client.ui' xmlns:b.html='urn:import:org.gwtbootstrap3.client.ui.html'
			 xmlns:p.client='urn:import:cz.metacentrum.perun.wui.registrar.client'
			 xmlns:g="urn:import:com.google.gwt.user.client.ui"
		>

	<ui:with field="res" type="cz.metacentrum.perun.wui.userprofile.client.resources.PerunUserProfileResources"/>

	<ui:style>
	</ui:style>

	<ui:with field="placeTokens" type="cz.metacentrum.perun.wui.userprofile.client.PerunUserProfilePlaceTokens"/>

	<b.html:Div addStyleNames="{res.gss.webContent}">

		<b:Navbar position="FIXED_TOP">
			<b:NavbarHeader ui:field="navbarHeader" addStyleNames="{res.gss.header}">
				<b:NavbarCollapseButton dataTarget="#navbar-collapse"/>
			</b:NavbarHeader>

			<b:NavbarCollapse b:id="navbar-collapse" ui:field="collapse">
				<g:FocusPanel ui:field="collapseClickHandler">
					<b:NavbarNav>
						<b:AnchorListItem icon="SIGN_OUT" ui:field="logout" targetHistoryToken="{placeTokens.getLogout}">Logout</b:AnchorListItem>
					</b:NavbarNav>
				</g:FocusPanel>
			</b:NavbarCollapse>
		</b:Navbar>

		<b.html:Div ui:field="pageContent" addStyleNames="{res.gss.pageWrapper}" />

	</b.html:Div>

</ui:UiBinder>